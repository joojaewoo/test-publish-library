{
  "name": "ssm-design-src",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "node scripts/build",
    "build:react": "node scripts/build --only react",
    "build:vue": "node scripts/build --only vue",
    "react": "npm run build:react && concurrently --kill-others \"vite ./playground/react\"",
    "vue": "npm run build:vue && concurrently --kill-others \"vite ./playground/vue\"",
    "storybook": "node scripts/build && start-storybook -p 6006",
    "run:storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish:prerelease-alpha": "cd ./src/copy/ && npm version prerelease --preid alpha && cd ../.. && npm run build && cd ./dist && npm publish",
    "publish:patch": "cd ./src/copy/ && npm version patch && cd ../.. && npm run build && cd ./dist && npm publish",
    "publish:minor": "cd ./src/copy/ && npm version minor && cd ../.. && npm run build && cd ./dist && npm publish",
    "publish:major": "cd ./src/copy/ && npm version major && cd ../.. && npm run build && cd ./dist && npm publish"
  },
